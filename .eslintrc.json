{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "tsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "no-console": "warn",
        "no-empty": "error",
        "camelcase": "error",
        "no-duplicate-imports": "error",
        "no-empty-function": "error",
        "no-var": "error",
        "eol-last": "error",
        "no-trailing-spaces": "error"
    }
}